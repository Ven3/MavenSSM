<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "//UNKNOWN/" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<!-- 别名 -->
	<typeAliases>
		<typeAlias type="com.jixiao.vo.AccountModel" alias="account" />
		<typeAlias type="com.jixiao.vo.CheckDocModel" alias="checkdoc" />
		<typeAlias type="com.jixiao.vo.CheckedAccountModel" alias="checkedaccount" />
		<typeAlias type="com.jixiao.vo.CheckValueModel" alias="checkvalue" />
		<typeAlias type="com.jixiao.vo.DeptModel" alias="dept" />
		<typeAlias type="com.jixiao.vo.DutyModel" alias="duty" />
		<typeAlias type="com.jixiao.vo.PendingDocModel" alias="pendingdoc" />
		<typeAlias type="com.jixiao.vo.RoleModel" alias="role" />
		<typeAlias type="com.jixiao.vo.Ta_Account_RoleModel" alias="ta_account_role" />
		<typeAlias type="com.jixiao.vo.Ta_Role_RightModel" alias="ta_role_right" />
		<typeAlias type="com.jixiao.vo.Tb_RightModel" alias="tb_right" />

		<typeAlias type="com.jixiao.vo.Kh_ResultModel" alias="kh_result" />
		<typeAlias type="com.jixiao.vo.Kh_LevelModel" alias="kh_level" />
		<typeAlias type="com.jixiao.vo.Kh_BigItemModel" alias="kh_bigitem" />
		<typeAlias type="com.jixiao.vo.Kh_SmallItemModel" alias="kh_smallitem" />
	</typeAliases>

	<!-- 数据库环境 -->
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost:3306/jixiao" />
				<property name="username" value="root" />
				<property name="password" value="root" />
			</dataSource>
		</environment>
	</environments>

	<!-- 表映射 -->
	<mappers>
		<mapper resource="com/jixiao/config/account.xml" />
		<mapper resource="com/jixiao/config/checkdoc.xml" />
		<mapper resource="com/jixiao/config/checkedaccount.xml" />
		<mapper resource="com/jixiao/config/checkvalue.xml" />
		<mapper resource="com/jixiao/config/dept.xml" />
		<mapper resource="com/jixiao/config/duty.xml" />
		<mapper resource="com/jixiao/config/pendingdoc.xml" />
		<mapper resource="com/jixiao/config/role.xml" />
		<mapper resource="com/jixiao/config/ta_account_role.xml" />
		<mapper resource="com/jixiao/config/ta_role_right.xml" />
		<mapper resource="com/jixiao/config/table.xml" />
		<mapper resource="com/jixiao/config/tb_right.xml" />

		<mapper resource="com/jixiao/config/kh_result.xml" />
		<mapper resource="com/jixiao/config/kh_level.xml" />
		<mapper resource="com/jixiao/config/kh_bigitem.xml" />
		<mapper resource="com/jixiao/config/kh_smallitem.xml" />
	</mappers>

</configuration> 