<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
	<constant name="struts.devMode" value="true" />

	<package name="default" namespace="/" extends="struts-default">

		<global-results>
			<result name="error" type="redirect">/file/error.jsp</result>
			<result name="input">/file/error.jsp</result>
		</global-results>

		<!-- 登录Action -->
		<action name="login" class="com.jixiao.action.LoginAction"
			method="login">
			<result name="success">/file/wdgz.jsp</result>
			<result name="fail">/file/error.jsp</result>
		</action>

		<!-- 待办工作Action -->
		<action name="dbgz_*" class="com.jixiao.action.DbgzAction"
			method="{1}">
			<result name="dbList">/file/wdgz_db.jsp</result>
			<result name="khAccountList">/file/wdgz_db1.jsp</result>
			<result name="insertKhAccount" type="redirect">/file/wdgz.jsp</result>
		</action>

		<!-- 起草考核Action -->
		<action name="qckh_*" class="com.jixiao.action.QckhAction"
			method="{1}">
			<result name="khList">/file/wdgz_qckh.jsp</result>
			<result name="afterInsert" type="redirectAction">qckh_khList</result>
		</action>

		<!-- 统计查询Action -->
		<action name="tjcx_*" class="com.jixiao.action.TjcxAction"
			method="{1}">
			<result name="tjList">/file/tjcx_tjcx.jsp</result>
			<result name="getResult">/file/tjcx_tjcx1.jsp</result>
			<result name="getFina">/file/tjcx_tjcx2.jsp</result>
		</action>

		<!-- 导出统计结果Action -->
		<action name="export" class="com.jixiao.action.ExportAction"
			method="export">
			<result name="export" type="stream">
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">attachment;filename="${filename}"</param>
				<param name="bufferSize">1024</param>
			</result>
		</action>

		<!-- 系统管理Action -->
		<action name="xtgl_*" class="com.jixiao.action.XtglAction"
			method="{1}">
			<result name="qxList">/file/xtgl_qxgl.jsp</result>
			<result name="qxUpdate">/file/xtgl_qxgl1.jsp</result>
			<result name="qxSave" type="redirectAction">xtgl_qxList.action</result>

			<result name="csList">/file/xtgl_csgl.jsp</result>
			<result name="csUpdate">/file/xtgl_csgl1.jsp</result>
			<result name="csSave" type="redirectAction">xtgl_csList.action</result>
		</action>

	</package>
</struts>